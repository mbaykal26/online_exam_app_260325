UPDATE sorular
SET secenekler = 'A) 1453;B) 1402;C) 1071'
WHERE id = 1;

-- 2. Soru: Sanayi Devrimi'nin sonuçlarından biri değildir?
UPDATE sorular
SET secenekler = 'A) Fabrikalarda seri üretim;B) Köyden kente göç hızlandı;C) Refah düzeyinde artış yaşandı'
WHERE id = 2;

-- 3. Soru: Batı etkisiyle ortaya çıkan eğitim kurumu
UPDATE sorular
SET secenekler = 'A) Enderun - Saray eğitimi;B) Medrese - Dini eğitim;C) Lonca - Mesleki eğitim'
WHERE id = 3;

-- 4. Soru: Descartes'in &quot;Düşünüyorum, öyleyse varım&quot; sözü
UPDATE sorular
SET secenekler = 'A) Rasyonalizm;B) Empirizm;C) Deneycilik;D) Duyumsamacılık'
WHERE id = 4;

-- 5. Soru: Herakleitos, felsefede varlığın değişmesi...
UPDATE sorular
SET secenekler = 'A) Herakleitos;B) Varlık yoktur, varlık oluştur;C) ?;D) ?'
WHERE id = 5;

-- 6. Soru: Varlığın son temelleri, özü ve...
UPDATE sorular
SET secenekler = 'A) Diyalektik;B) Logos;C) ?;D) ?'
WHERE id = 6;


SELECT id, soru_metni, secenekler FROM sorular;
